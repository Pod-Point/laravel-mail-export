{
    "name": "pod-point/laravel-mail-export",
    "description": "Simple package to export emails to S3",
    "keywords": ["laravel", "kinesis", "monolog", "logging"],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.3",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pod Point Software Team",
            "email": "software@pod-point.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "PodPoint\\LaravelMailExport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PodPoint\\LaravelMailExport\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
