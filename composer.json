{
    "name": "pod-point/laravel-mail-export",
    "description": "A mailable trait to export all or individual mails to a storage disk",
    "keywords": ["laravel", "Mail"],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.3",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pod Point Software Team",
            "email": "software@pod-point.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/filesystem": "^5.4",
        "illuminate/mail": "^5.4",
        "illuminate/support": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "PodPoint\\LaravelMailExport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PodPoint\\LaravelMailExport\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MailExportServiceProvider"
            ]
        }
    }
}
